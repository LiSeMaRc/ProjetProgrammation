import unittest 
from graph import Graph
from grid import Grid

class Test_bfs(unittest.TestCase):


    def test_graph1_graph(self):
        graph = Graph.graph_from_file("input/graph1.in")
        lengths=[]
        count=0
        file=open("C:\\Users\\lisem\\OneDrive\\Documents\\ENSAE\\1A\\ensae-prog24\\input\\graph1.path.out", "r")
        for line in file:
            elements=line.split()
            lengths.append(elements[2])
        for i in range(1,20):
            for j in range(i+1,21):
                self.assertEqual(len(graph.bfs(i,j)), lengths[count])
                count=count+1



if __name__ == '__main__':
    unittest.main()
        

        